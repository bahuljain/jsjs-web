{
  "name": "jsjs-frontend",
  "version": "0.0.1",
  "description": "The web front-end for the JSJS language.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watchify js/app.js -t babelify -o dist/bundle.js -v",
    "deploy": "npm run build && surge",
    "build": "NODE_ENV=production ./node_modules/.bin/browserify js/app.js -t babelify | uglifyjs -c > dist/bundle.js"
  },
