  "bugs": {
    "url": "https://github.com/bahuljain/jsjs-web/issues"
  },
  "homepage": "https://github.com/bahuljain/jsjs-web#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "cash-dom": "^1.3.0",
    "uglify-js": "^2.7.4",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "jquery": "^2.2.3"
  }
